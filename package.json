{
  "name": "big-site",
  "private": true,
  "description": "Bare-bones starter.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE_BUNDLE_SIZE=true gatsby build",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@mui/material": "^5.2.8",
    "d3-scale": "^3.3.0",
    "gatsby": "^4.7.1",
    "gatsby-background-image": "^1.6.0",
    "gatsby-image": "^3.1.0",
    "gatsby-plugin-google-gtag": "^4.7.0",
    "gatsby-plugin-image": "^2.7.0",
    "gatsby-plugin-mdx": "^3.7.0",
    "gatsby-plugin-react-helmet": "^5.7.0",
    "gatsby-plugin-sass": "^5.7.0",
    "gatsby-plugin-sharp": "^4.13.0",
    "gatsby-remark-embed-video": "^3.1.1",
    "gatsby-remark-mermaid": "^2.1.0",
    "gatsby-source-filesystem": "^4.13.0",
    "gatsby-transformer-json": "^4.13.0",
    "gatsby-transformer-remark": "^5.13.0",
    "gatsby-transformer-sharp": "^4.13.0",
    "prismjs": "^1.26.0",
    "puppeteer": "^13.3.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.49.7"
  },
  "devDependencies": {
    "babel-preset-gatsby": "^1.1.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "gatsby-remark-autolink-headers": "^5.7.0",
    "gatsby-remark-copy-linked-files": "^5.7.0",
    "gatsby-remark-images": "^6.7.0",
    "gatsby-remark-prismjs": "^6.13.0",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imagineLife/big-site.git"
  },
  "jest": {
    "testRegex": [
      "(/__tests__/.*|(\\.|/)(test))\\.[jt]sx?$"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/enzyme.setup.js"
    ]
  }
}

{
  "name": "big-site",
  "private": true,
  "description": "Bare-bones starter.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE_BUNDLE_SIZE=true gatsby build",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@mui/material": "^5.8.7",
    "@netlify/build-info": "^6.0.7",
    "d3-scale": "^3.3.0",
    "gatsby": "^5.0.0",
    "gatsby-background-image": "^1.6.0",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-google-gtag": "^4.18.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-manifest": "^5.2.0",
    "gatsby-plugin-mdx": "^5.2.0",
    "gatsby-plugin-no-sourcemaps": "^5.2.0",
    "gatsby-plugin-react-helmet": "^6.2.0",
    "gatsby-plugin-react-helmet-async": "^1.2.2",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sass": "^6.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-plugin-sitemap": "^6.2.0",
    "gatsby-remark-autolink-headers": "^6.2.0",
    "gatsby-remark-copy-linked-files": "^6.2.0",
    "gatsby-remark-embed-video": "^3.2.1",
    "gatsby-remark-images": "^7.2.0",
    "gatsby-remark-mermaid": "^3.0.1",
    "gatsby-remark-prismjs": "^7.2.0",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-json": "^5.2.0",
    "gatsby-transformer-remark": "^6.2.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "prismjs": "^1.28.0",
    "puppeteer": "^15.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-preset-gatsby": "^2.18.0",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imagineLife/big-site.git"
  },
  "jest": {
    "testRegex": [
      "(/__tests__/.*|(\\.|/)(test))\\.[jt]sx?$"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/enzyme.setup.js"
    ]
  }

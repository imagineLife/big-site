{
  "name": "big-site",
  "private": true,
  "description": "Bare-bones starter.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE_BUNDLE_SIZE=true gatsby build",
    "build": "gatsby build",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "d3-scale": "^3.3.0",
    "gatsby": "^3.0.0",
    "gatsby-background-image": "^1.5.0",
    "gatsby-image": "^3.1.0",
    "gatsby-plugin-mdx": "^2.0.1",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sass": "^4.0.2",
    "gatsby-plugin-sharp": "^3.1.1",
    "gatsby-remark-mermaid": "^2.1.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-json": "^3.1.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.1.0",
    "prismjs": "^1.25.0",
    "puppeteer": "^10.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "babel-preset-gatsby": "^1.1.0",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "gatsby-remark-autolink-headers": "^4.5.0",
    "gatsby-remark-copy-linked-files": "^4.5.0",
    "gatsby-remark-images": "^5.5.0",
    "gatsby-remark-prismjs": "^5.11.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imagineLife/big-site.git"
  },
  "jest": {
    "testRegex": [
      "(/__tests__/.*|(\\.|/)(test))\\.[jt]sx?$"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/enzyme.setup.js"
    ]
  }
}
